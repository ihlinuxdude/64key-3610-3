#include <dt-bindings/zmk/matrix_transform.h>
#include <physical_layouts.dtsi>
/ {
    chosen {
       
   zmk,physical-layout = &charybdis_nano;
      
    };

   default_transform: keymap_transform_0 {
        compatible = "zmk,matrix-transform";
		columns = <12>;
		rows = <6>;

		map = <
RC(0,0) RC(0,1) RC(0,2) RC(0,3) RC(0,4) RC(0,5)  RC(0,6)  RC(0,7) RC(0,8) RC(0,9) RC(0,10) RC(0,11)
RC(1,0) RC(1,1) RC(1,2) RC(1,3) RC(1,4) RC(1,5)  RC(1,6)  RC(1,7) RC(1,8) RC(1,9) RC(1,10) RC(1,11)
RC(2,0) RC(2,1) RC(2,2) RC(2,3) RC(2,4) RC(2,5)  RC(2,6)  RC(2,7) RC(2,8) RC(2,9) RC(2,10) RC(2,11)
RC(3,0) RC(3,1) RC(3,2) RC(3,3) RC(3,4) RC(3,5)  RC(3,6)  RC(3,7) RC(3,8) RC(3,9) RC(3,10) RC(3,11)
	        RC(4,2) RC(4,3) RC(4,4) RC(4,5)  RC(4,6)  RC(4,7) RC(4,8) RC(4,9) 
                RC(5,2) RC(5,3) RC(5,4) RC(5,5)  RC(5,6)  RC(5,7) RC(5,8)  RC(5,9) 
		>;
    };

   kscan0: kscan {        
		compatible = "zmk,kscan-gpio-matrix";
		label = "KSCAN";

	
		diode-direction = "col2row";
		       row-gpios= <&pro_micro 19 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
		                 , <&pro_micro 18 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)> // D4
				, <&pro_micro 15 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)> // C6
				, <&pro_micro 14 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)> // D7
				, <&pro_micro 16 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)> // D7
				, <&pro_micro 10 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)> // E6
				;
	};

charybdis_nano: charybdis_nano {
